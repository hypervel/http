{
    "name": "laravel-hyperf/http",
    "type": "library",
    "description": "The http package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "swoole",
        "http",
        "laravel-hyperf"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\Http": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "hyperf/http-server": "~3.1.0",
        "hyperf/collection": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/codec": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "nesbot/carbon": "^2.0 || ^3.0"
    },
    "suggest": {
        "hyperf/session": "Required to `session` function in request.",
        "hyperf/validation": "Required to `validate` function in request."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "LaravelHyperf\\Http\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}